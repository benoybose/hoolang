    "version": "0.2.0",
    "configurations": [
        {
            "name": "MingW (hooc_test)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/hooc/hooc_test.exe",
            "args": ["--run_test=X86EmitterTest/TEST14"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}